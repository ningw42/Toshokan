{
    "manifest_version": 2,
    "name": "DoubanFav",
    "version": "1.0",
    "description": "Add book to wishlist",
    "icons": {
        "16": "images/logo.png",
        "48": "images/logo.png",
        "128": "images/logo.png"
    },
    "browser_action": {
        "default_icon": {
            "19": "images/logo.png",
            "38": "images/logo.png"
        },
        "default_title": "DoubanFav"
    },
    "background": {
      "scripts": ["jquery.min.js", "background.js"]
    },
    "permissions": ["http://book.douban.com/*", "http://zhangteng.yifangyun.com/*"]
}
